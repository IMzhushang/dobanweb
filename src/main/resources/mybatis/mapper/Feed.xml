<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doban.mapper.FeedMapper">
     <insert id="publish" parameterType="com.doban.domain.Feed" >
         INSERT INTO tab_feed (tab_feed.owerId,tab_feed.content,tab_feed.time) VALUES(#{owerID},#{content},#{time}})
     </insert>
</mapper>